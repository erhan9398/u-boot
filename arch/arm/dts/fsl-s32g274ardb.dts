/dts-v1/;
#include "fsl-s32g274a.dtsi"

&usdhc0 {
	status = "okay";
};

&dspi1 {
	status = "okay";
};

&dspi5 {
	status = "okay";
};

&gmac0 {
	status = "okay";
	phy-mode = "rgmii";
	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&pfe {
	status = "okay";
};

/* IO Expander  */
&i2c0 {
	clock-frequency=<100000>;
	status = "okay";
};

/* PCIe X1 Connector  */
&i2c1 {
	clock-frequency=<100000>;
	status = "okay";
};

/* FLEXRAY_LIN  */
&i2c2 {
	clock-frequency=<100000>;
	status = "okay";
};

/* PMIC */
&i2c4 {
	clock-frequency=<100000>;
	status = "okay";

	vr5510 {
		compatible = "fsl,vr5510";
		reg = <0x20>;
		status = "okay";
	};

	vr5510_fsu {
		compatible = "fsl,vr5510";
		reg = <0x21>;
		status = "okay";
	};
};
